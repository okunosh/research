import numpy as np
import matplotlib.pyplot as plt

g = 9.81
gamma = 3.e-3
theta_0 = 288
N = np.sqrt(gamma * g / theta_0)

deg2rad = np.pi / 180
alpha = np.linspace(0, 90, 1000)

N_alpha = N * np.sin(alpha*deg2rad)


plt.plot(alpha, N_alpha)
plt.yscale('log')
plt.xlabel('$\alpha$')
plt.ylabel('$\N_alpha')
plt.grid()
plt.show()
